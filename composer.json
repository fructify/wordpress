{
	"name":"fructify/wordpress",
	"type":"project",
	"description": "Making WordPress Fruitful and Productive",
	"homepage": "https://github.com/fructify/wordpress",
	"keywords": ["fructify", "wordpress", "composer", "laravel"],
	"license": "MIT",
	"repositories":
	[{
		"type": "composer",
		"url": "http://wpackagist.org"
	}],
	"require":
	{
		"codegyre/robo": "*",
		"wp-cli/wp-cli": "*",
		"fructify/robo": "*",
		"fructify/theme": "*"
	},
	"scripts":
	{
		"post-install-cmd":
		[
			"robo fructify:install",
			"robo fructify:permissions",
			"robo fructify:salts"
		],
		"post-update-cmd": "robo fructify:update"
	}
}